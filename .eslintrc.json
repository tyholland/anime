{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "react/no-unescaped-entities": "off",
    "jsx-quotes": ["error", "prefer-double"],
    "react/prop-types": 0,
    "indent": [
      "error",
      2,
      {
        "ignoredNodes": ["TemplateLiteral"]
      }
    ],
    "eol-last": ["error", "always"]
  }
}
