{
  "matchup": {
    "display": "flex",
    "flexDirection": "row",
    "justifyContent": "space-between"
  },
  "section": {
    "display": "flex",
    "flexDirection": "row",
    "alignItems": "center",
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 80
  },
  "duoSection": {
    "display": "flex",
    "flexDirection": "row",
    "alignItems": "center",
    "height": 80
  },
  "positionSection": {
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 80
  },
  "duoPositionSection": {
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 160
  },
  "headliner": {
    "width": "50%"
  },
  "teamSplit": {
    "borderTopWidth": 1,
    "borderStyle": "solid",
    "borderTopColor": "#000",
    "display": "flex",
    "flexDirection": "row",
    "alignItems": "center",
    "width": "100%",
    "marginTop": 30
  },
  "column": {
    "width": "46%"
  },
  "positionColumn": {
    "width": "8%"
  },
  "position": {
    "width": "100%",
    "paddingVertical": 17,
    "fontWeight": "600",
    "color": "#999",
    "textAlign": "center",
    "backgroundColor": "rgba(0, 0, 0, .1)",
    "fontSize": 14,
    "height": "100%",
    "paddingTop": 30
  },
  "duoPosition": {
    "width": "100%",
    "paddingVertical": 17,
    "fontWeight": "600",
    "color": "#999",
    "textAlign": "center",
    "backgroundColor": "rgba(0, 0, 0, .1)",
    "fontSize": 12,
    "height": "100%",
    "paddingTop": 70
  },
  "character": {
    "marginTop": 5,
    "marginLeft": "10%"
  },
  "characterReverse": {
    "marginLeft": 0,
    "marginRight": "10%",
    "textAlign": "right"
  },
  "affinity": {
    "marginTop": 15,
    "marginLeft": "10%"
  },
  "affinityReverse": {
    "marginLeft": 0,
    "marginRight": "5%",
    "position": "relative",
    "right": "-75%"
  },
  "affinityReverseEmpty": {
    "marginLeft": 0,
    "marginRight": "10%",
    "textAlign": "right"
  },
  "power": {
    "width": "50%",
    "textAlign": "right",
    "marginVertical": 15,
    "paddingRight": "5%",
    "color": "rgb(27, 5, 246)"
  },
  "powerReverse": {
    "textAlign": "left",
    "paddingRight": 0,
    "paddingLeft": "5%"
  },
  "teamContent": {
    "width": "50%"
  },
  "teamName": {
    "fontSize": 18,
    "fontWeight": "700",
    "textAlign": "center"
  },
  "teamTotal": {
    "fontSize": 14,
    "color": "rgba(0, 0, 0, .5)",
    "textAlign": "center",
    "marginTop": 15
  },
  "reverse": {
    "flexDirection": "row-reverse"
  }
}
