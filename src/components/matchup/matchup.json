{
  "column": {
    "width": "46%"
  },
  "section": {
    "display": "flex",
    "flexDirection": "row",
    "alignItems": "center",
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 80
  },
  "duoSection": {
    "display": "flex",
    "flexDirection": "row",
    "alignItems": "center",
    "height": 80
  },
  "headliner": {
    "width": "50%"
  },
  "character": {
    "marginTop": 5,
    "marginLeft": "10%"
  },
  "affinity": {
    "marginTop": 15,
    "marginLeft": "10%"
  },
  "power": {
    "width": "50%",
    "marginVertical": 15,
    "paddingRight": "5%"
  },
  "powerText": {
    "textAlign": "right",
    "color": "rgb(27, 5, 246)"
  },
  "reverseSection": {
    "display": "flex",
    "flexDirection": "row-reverse",
    "alignItems": "center",
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 80
  },
  "reverseDuoSection": {
    "display": "flex",
    "flexDirection": "row-reverse",
    "alignItems": "center",
    "height": 80
  },
  "reverseCharacter": {
    "marginTop": 5,
    "marginLeft": 0,
    "marginRight": "10%",
    "textAlign": "right"
  },
  "reverseAffinity": {
    "marginTop": 15,
    "marginLeft": 0,
    "marginRight": "5%",
    "position": "relative",
    "right": "-75%"
  },
  "reversePower": {
    "width": "50%",
    "marginVertical": 15,
    "paddingRight": 0,
    "paddingLeft": "5%"
  },
  "reversePowerText": {
    "textAlign": "left",
    "color": "rgb(27, 5, 246)"
  }
}
