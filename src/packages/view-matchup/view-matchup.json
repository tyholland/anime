{
  "matchup": {
    "display": "flex",
    "flexDirection": "row",
    "justifyContent": "space-between"
  },
  "duoSection": {
    "display": "flex",
    "flexDirection": "row",
    "alignItems": "center",
    "height": 80
  },
  "positionSection": {
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 80
  },
  "duoPositionSection": {
    "borderBottomWidth": 1,
    "borderStyle": "solid",
    "borderBottomColor": "#000",
    "height": 160
  },
  "teamSplit": {
    "borderTopWidth": 1,
